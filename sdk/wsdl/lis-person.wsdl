        <xs:complexType name="Person.Type">
            <xs:annotation>
                <xs:documentation source="umlcommentbox">
                    The Person data-type is the top-level container for the Person data model.
                    The data model consists of:
                    
                    (1) Zero or more formatted names;
                    (2) Zero or more partenised names;
                    (3) Zero or more addresses;
                    (4) Zero or more contact information sets;
                    (5) Zero or more demographic sets;
                    (6) Zero or more agents;
                    (7) Zero or more roles within the Enterprise;
                    (8) An optional extension point.
                </xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element ref="tns:formname" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="tns:name" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="tns:address" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="tns:contactinfo" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="tns:demographics" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="tns:agent" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="tns:roles" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="tns:extension" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>

        <xs:complexType name="PersonCore.Type">
            <xs:annotation>
                <xs:documentation source="umlcommentbox">
                    The PersonCore data-type is the top level container for the mandatory 
                    aspects of the Person data-model.  The data model consists of:
                    
                    (1) SourcedId;
                    (2) Formatted name;
                    (3) UserId.
                </xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element ref="tns:sourcedId" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="tns:formname" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="tns:userId" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>

        <xs:complexType name="PersonRecord.Type">
            <xs:annotation>
                <xs:documentation source="umldocumentation">
                    The PersonRecord complexType is the container for the collation of the Person data structure
and its sourcedGUID.
                </xs:documentation>
            </xs:annotation>
            <xs:sequence>
                <xs:element ref="tns:sourcedGUID" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="tns:person" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>

        <xs:complexType name="SourcedGUID.Type">
            <xs:sequence>
                <xs:element ref="tns:refAgentInstanceID" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="tns:sourcedId" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>

        <xs:complexType name="PersonRecordSet.Type">
            <xs:sequence>
                <xs:element ref="tns:personRecord" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>

